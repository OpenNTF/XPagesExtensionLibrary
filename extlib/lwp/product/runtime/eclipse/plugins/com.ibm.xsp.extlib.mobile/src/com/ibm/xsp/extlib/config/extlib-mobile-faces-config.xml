            <component-family>javax.faces.Input</component-family>
            <renderer-type>com.ibm.xsp.extlib.mobile.TypeAheadInput.ReadOnly</renderer-type>
            <renderer-class>com.ibm.xsp.renderkit.html_basic.ReadOnlyValueRenderer</renderer-class>
