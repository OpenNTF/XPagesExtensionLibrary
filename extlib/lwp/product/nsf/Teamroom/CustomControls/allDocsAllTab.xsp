			<xp:dominoView var="view1" viewName="xvwDocsByActiveDate"
				expandLevel="1" dataCache="full">
				<xp:this.categoryFilter><![CDATA[#{javascript://
if( sessionScope.filterEnabled ){
	return sessionScope.filterColumnValue;
}												
}]]></xp:this.categoryFilter>
			</xp:dominoView>
		</xe:this.data>
		<xe:this.summaryColumn>
			<xe:viewSummaryColumn columnName="$143"
				columnTitle="Title">
			</xe:viewSummaryColumn>
		</xe:this.summaryColumn>
		
		<xe:this.extraColumns>
